{
  "name": "apie/primary-key-plugin",
  "description": "Composer package of the apie library: primary key plugin",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\PrimaryKeyPlugin\\": "src/"
